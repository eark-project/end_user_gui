@model end_user_gui.Models.Order

@{
    Layout = "SelectViewer.cshtml";
    ViewBag.KeyColumn = "orderId";
    ViewBag.Id = Model.OrderUniqueID;
    ViewBag.SpanPrefix = "selArch_";
    ViewBag.Controller = "Orders";
    ViewBag.Action = "SelectArchivist";
    ViewBag.IsNull = Model.Archivist == null ;
}

@section Display{
    @Html.DisplayFor(modelItem => Model.Archivist.Name)
}