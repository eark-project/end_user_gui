
@model end_user_gui.Models.Order

@Html.DisplayFor(modelItem => Model.Archivist.Name)
@{ if (Model.Archivist == null)
    {
        Output.Write("(none)");
    }
}

@Ajax.ActionLink("(Change)",
    "SelectArchivist",
    "Orders",
    new { orderId = Model.OrderUniqueID },
    new AjaxOptions()
    {
        UpdateTargetId = ViewData["elementId"].ToString()
    }
    )