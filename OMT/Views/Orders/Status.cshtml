
@model end_user_gui.Models.Order
@{
    Layout = "SelectViewer.cshtml";
    ViewBag.KeyColumn = "orderId";
    ViewBag.Id = Model.OrderUniqueID;
    ViewBag.SpanPrefix = "selStat_";
    ViewBag.Controller = "Orders";
    ViewBag.IsNull = Model.Status == null || Model.Status.Status == null;
}

@section Display{
    @Html.DisplayFor(modelItem => Model.Status.Status)
    @Html.DisplayFor(modelItem => Model.Status.StatusDate)
}
