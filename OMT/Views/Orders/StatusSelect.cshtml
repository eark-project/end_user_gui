@model end_user_gui.Models.Order

@{ 
    Layout = "SelectSelector.cshtml";
    ViewBag.KeyColumn = "orderId";
    ViewBag.Column = "statusId";
    ViewBag.Id = Model.OrderUniqueID;
    ViewBag.SpanPrefix = "selStat_";
    ViewBag.Controller = "Orders";
    ViewBag.Action = "SetStatus";
}

@section Select{
    @Html.EnumDropDownListFor(
    o => o.Status.Status,
    new { onchange = "updateSelectSaveLink(this, 'statusId')" }
    )
}