@using end_user_gui.Models
@using end_user_gui.App_GlobalResources
@model Order
<nav class="navbar navbar-default">
    <div class="container-fluid">
        <div class="navbar-header">
            <a class="navbar-brand">@Messages.navbar_brand</a>
        </div>
        <div>
            <ul class="nav navbar-nav">
                <li class="active"><a href="/">@Messages.navbar_home</a></li>
                <li class="active"><a href="/profile">@Messages.navbar_profile</a></li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li class="active">
                    <button type="button" class="btn btn-link btn-lg" data-toggle="modal" data-target="#cartModal" data-backdrop="static" name="cartViewButton">
                        @Messages.cart_view
                        <span name="cartCount"></span>
                    </button>
                </li>
            </ul>
        </div>
    </div>
</nav>
@{
    Html.RenderPartial("~/Views/Order/cartview.cshtml", Model);
    Html.RenderPartial("~/Views/Order/addtocartmodal.cshtml", new Tuple<Archive, List<Dissemination>>(null, null));
}
